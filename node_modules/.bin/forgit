#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/arthurvienotongaro/Developer/projets/forgit/node_modules/.pnpm/forgit@1.0.0/node_modules/forgit/bin/node_modules:/Users/arthurvienotongaro/Developer/projets/forgit/node_modules/.pnpm/forgit@1.0.0/node_modules/forgit/node_modules:/Users/arthurvienotongaro/Developer/projets/forgit/node_modules/.pnpm/forgit@1.0.0/node_modules:/Users/arthurvienotongaro/Developer/projets/forgit/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/arthurvienotongaro/Developer/projets/forgit/node_modules/.pnpm/forgit@1.0.0/node_modules/forgit/bin/node_modules:/Users/arthurvienotongaro/Developer/projets/forgit/node_modules/.pnpm/forgit@1.0.0/node_modules/forgit/node_modules:/Users/arthurvienotongaro/Developer/projets/forgit/node_modules/.pnpm/forgit@1.0.0/node_modules:/Users/arthurvienotongaro/Developer/projets/forgit/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/forgit@1.0.0/node_modules/forgit/bin/index.js" "$@"
else
  exec node  "$basedir/../.pnpm/forgit@1.0.0/node_modules/forgit/bin/index.js" "$@"
fi
